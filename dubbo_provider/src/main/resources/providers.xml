<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="dubbo-provider" owner="dubbo-provider"/>
    <dubbo:registry address="zookeeper://127.0.0.1:2181" check="false" subscribe="false"/>
    <dubbo:service interface="com.example.demo.service.TestService" ref="testService"/>
    <dubbo:service interface="com.example.demo.service.PersonService" ref="personService"/>
    
    <bean id="testService" class="com.example.service.impl.TestServiceImpl"></bean>
 	<bean id="personService" class="com.example.service.impl.PersonServiceImpl"></bean> 
    
</beans>